<script>
   
  function addcartItems(id){
   $.ajax({
  url:'/cartPro',
  data:{
    productId:id
  },
  method:'post',
  success:(response)=>{
    if(response.added){
     Toastify({
          text: "item added to cart",
          duration: 3000,
          gravity: "bottom",
          position: "center",  
          style: {
              background: '#000000',
             
          },
     }).showToast();}
     else{
      Toastify({
          text: "Please Login your account",
          duration: 3000,
          gravity: "top",
          position: "center",  
          style: {
              background: '#000000',
             
          },
     }).showToast();}

     }
   
  
          
})
  }
 
        
        function wished(id){  
            $.ajax({
        url:'/wishlist',
        data:{
          productId:id
        },
        method:'post',
        success:(response)=>{
          if(response.status){
                       Toastify({
                text: "Added to wishlist",
                duration: 3000,
                gravity: "bottom",
                position: "center",  
                style: {
                    background: '#000000',
                   
                },
           }).showToast();}else{
            Toastify({
                text: "Please Login your account",
                duration: 3000,
                gravity: "top",
                position: "center",  
                style: {
                    background: '#000000',
                   
                },
           }).showToast();

           }
        
                }
      })
        }
       
  </script>

