
    <section class="content-main">
    <div class="content-header">
      <div>
        <h2 class="content-title card-title">Users List</h2>
        <p>Users registered with Flexcart</p>
      </div>
      <div>
        <a href="#" class="btn btn-light rounded font-md">Block</a>
        <a href="#" class="btn btn-light rounded font-md">Unblock</a>
        <a href="#" class="btn btn-primary btn-sm rounded">Delete</a>
      </div>
    </div>
    <div class="card mb-4">
      <div class="card-body">
        <article class="itemlist">
            <%data.forEach(function(userdetail) { %>
          <div class="row align-items-center">
            <div class="col col-check flex-grow-0">
              <div class="form-check">
                <input class="form-check-input" type="checkbox" />
              </div>
            </div>
            <div class="col-lg-4 col-sm-4 col-8 flex-grow-1 col-name">
              <a class="itemside" href="#">
                <div class="left">
                  <img
                    src="/imgs/items/1.jpg"
                    class="img-sm img-thumbnail"
                    alt="Item"
                  />
                </div>
                <div class="info">
                  <p class="mb-0"><%= userdetail.username %></p>
                </div>
                <div class="info">
                  <p class="mb-0"><%=userdetail.email%></p>
                </div>
                <div class="info">

                    
                  <p class="mb-0"><%=userdetail.mobile%></p>
                </div>
              </a>
            </div>

            <div class="col-lg-2 col-sm-2 col-4 col-action text-end">
              <a href="#" class="btn btn-sm font-sm rounded btn-brand">
                <i class="material-icons md-edit"></i> Edit
              </a>
              <a href="#" class="btn btn-sm font-sm btn-light rounded">
                <i class="material-icons md-delete_forever"></i> Delete
              </a>
            </div>
          </div>
          <% }); %> 
          <!-- row .// -->
        </article>

        <!-- itemlist  .// -->
      </div>
      <!-- card-body end// -->
    </div>
    <!-- card end// -->
    

